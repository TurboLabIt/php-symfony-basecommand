{
    "name": "turbolabit/php-symfony-basecommand",
    "description": "A minimal Symfony bundle to build your own CLI commands even faster",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "TurboLabIt\\BaseCommand\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TurboLabIt\\BaseCommand\\tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Zane"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "@stable",
        "symfony/lock": "@stable",
        "league/csv": "@stable",
        "symfony/string": "@stable",
        "symfony/translation-contracts": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "symfony/dependency-injection": "^6.3",
        "symfony/config": "^6.3",
        "symfony/http-kernel": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
